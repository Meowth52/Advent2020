<Window x:Class="Advent2020.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Advent2020"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="DayBox" HorizontalAlignment="Left" Height="33" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="61" FontFamily="Global Monospace" KeyUp="DayBoxKeyUp"/>
        <TextBox x:Name="InputBox" HorizontalAlignment="Left" Height="69" Margin="89,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="460" FontFamily="Global Monospace" KeyUp="InputBoxKeyUp"/>
        <TextBox x:Name="OutputBox" HorizontalAlignment="Left" Height="313" Margin="89,96,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="693" FontFamily="Global Monospace"/>

    </Grid>
</Window>
